	<queryString>
		<![CDATA[SELECT distinct
   regimen.regimeesquema,
   regimen.codigoregime,
   regimen.report_order,
   count(distinct regimen.contagem) contagem,
   count(distinct regimen.uscontagem) uscontagem,
   count(distinct regimen.dccontagem) dccontagem,
   count(distinct regimen.pacienteprep) totalpacientesprep
FROM
   (
 SELECT  distinct p.patient,
 pat.patientid,
 rt.regimeesquema,
 rt.codigoregime,
 l.linhanome,
 rt.report_order,
 CASE WHEN ( pdit.flagidentifier = 'Nao' and pds.amount <> 0 and p.ppe = 'F' ) THEN pack.id END contagem,
 CASE WHEN ( pdit.flagidentifier = 'Sim' and pds.amount <> 0) THEN pack.id END uscontagem,
 CASE WHEN ( pdit.flagidentifier = 'Nao' and pds.amount = 0) THEN pack.id END dccontagem,
 CASE WHEN ( p.tipodoenca = 'PREP') THEN p.id END pacienteprep,
 ep.startreason,
 ep.startdate
 FROM   (
	 select distinct max(pre.date) predate,
		max(pa.pickupdate) pickupdate,
		max(pat.dateofbirth) dateofbirth,
		max(pa.weekssupply) weekssupply,
	 	pat.id,
		visit.episodedate
	 from package pa
	 inner join packageddrugs pds on pds.parentpackage = pa.id
	 inner join prescription pre on pre.id = pa.prescription
     inner join patient pat ON pre.patient=pat.id
	 INNER JOIN (
  			SELECT MAX (startdate) episodedate,
  				patient
  			from episode
  			WHERE startdate <= $P{dateEnd}::date
  			GROUP BY 2) visit on visit.patient = pat.id
	 where pg_catalog.date(pa.pickupdate) >= $P{date}::date
	 and pg_catalog.date(pa.pickupdate) <=  $P{dateEnd}::date
	 and (pre.tipodoenca like '%ARV%' OR pre.tipodoenca like '%PREP%')
	 GROUP BY 5,6 order by 5
	) pack
	 inner join prescription p on p.date::date = pack.predate::date and p.patient=pack.id
	 inner join patient pat on pat.id = pack.id
	 inner join package pa on pa.prescription = p.id and pa.pickupdate = pack.pickupdate
	 inner join packageddrugs pds on pds.parentpackage = pa.id
	 inner join packagedruginfotmp pdit on pdit.packageddrug = pds.id
	 INNER JOIN (
  			SELECT Max(id) visitid, startdate,
  				patient
  			from episode
  			WHERE startdate <= $P{dateEnd}::date
  			GROUP BY 2,3) ep1 on ep1.patient = pat.id and ep1.startdate = pack.episodedate
  	 inner join episode ep on ep.id = ep1.visitid
	 inner join regimeterapeutico rt on rt.regimeid = p.regimeid
	 inner join linhat l on l.linhaid = p.linhaid
	 where (p.tipodoenca like '%ARV%' OR p.tipodoenca like '%PREP%') and ep.startreason not like '%nsito%' and ep.startreason not like '%ternidade%' and ep.startreason not like '%CCR%'
	 ) regimen
group by
   1,
   2,
   3
order by
 regimen.report_order asc]]>
	</queryString>
